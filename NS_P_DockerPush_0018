resources:
  - name: dbp_repo
    type: GitRepo
    configuration:
      gitProvider: raksha78_github                     
      path: raksha78/windowsimage  
      branches:
        include: master

pipelines:
  - name: NS_P_DockerPush_0018
    configuration:
      nodePool: win_2019
    steps:
      - name: NS_P_DockerPush_0018_01
        type: DockerBuild
        configuration:
          affinityGroup: NS_P_DockerPush_0018
          dockerFileLocation: .
          dockerFileName: Dockerfile
          dockerImageName: dev.jfrog-pipelines.com/rak_docker/img
          dockerImageTag: ${run_number}
          integrations:
            - name: art_v
          inputResources:
            - name: dbp_repo
            
      - name: NS_P_DockerPush_0018_02
        type: DockerPush
        execution:
          onExecute:
            - echo "executing task command 1"
        configuration:
          affinityGroup: NS_P_DockerPush_0018
          targetRepository: rak_docker 
          integrations:
            - name: art_v 
          inputSteps:
            - name: NS_P_DockerPush_0018_01  
          
